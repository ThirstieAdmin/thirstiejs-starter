{
  "name": "thirstiejs-starter",
  "version": "1.3.1",
  "description": "A simple web app using the Thirstie Ecomm-vue SDK",
  "homepage": "https://docs.thirstie.com/",
  "repository": "github:ThirstieAdmin/thirstiejs-starter",
  "scripts": {
    "start": "dotenv -e .env.local -- parcel src/*.html --lazy --no-cache",
    "clean": "concurrently \"recursive-delete 'dist'\" \"recursive-delete '.parcel-cache'\"",
    "dev": "parcel src/*.html --lazy --no-cache",
    "dev:local": "dotenv -e .env.local npm -- run dev"
  },
  "author": "Thirstie, Inc.",
  "license": "MIT",
  "dependencies": {
    "@thirstie/ecomm-vue": "^1.3.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "parcel": "^2.15.4",
    "recursive-fs": "^2.1.0",
    "dotenv-cli": "^8.0.0"
  }
}
